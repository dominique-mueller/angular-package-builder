{
	"compilerOptions": {
		"declaration": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": [
			"dom",
			"es2015",
			"es2016",
			"es2017",
			"esnext"
		],
		"module": "es2015",
		"moduleResolution": "node",
		"sourceMap": true,
		"stripInternal": true,
		"typeRoots": [
			"node_modules/@types"
		],
		"types": []
	},
	"files": [],
	"angularCompilerOptions": {
		"annotateForClosureCompiler": true,
		"skipTemplateCodegen": true,
		"strictMetadataEmit": true
	}
}
