{
  "pull_request": {
    "subject_cannot_be_empty": true,
    "subject_must_be_longer_than": 4,
    "subject_must_be_shorter_than": 101,
    "subject_must_be_in_tense": "imperative",
    "subject_must_include_prefix": {
      "prefixes": [
        "feat",
        "fix",
        "docs",
        "refactor",
        "perf",
        "styles",
        "test",
        "chore",
        "[WIP] feat",
        "[WIP] fix",
        "[WIP] docs",
        "[WIP] refactor",
        "[WIP] perf",
        "[WIP] styles",
        "[WIP] test",
        "[WIP] chore",
        "[BLOCKED] feat",
        "[BLOCKED] fix",
        "[BLOCKED] docs",
        "[BLOCKED] refactor",
        "[BLOCKED] perf",
        "[BLOCKED] styles",
        "[BLOCKED] test",
        "[BLOCKED] chore"
      ],
      "require_after_prefix": "(*): "
    },
    "body_cannot_be_empty": true
  },
  "issue": {
    "subject_cannot_be_empty": true,
    "subject_must_be_longer_than": 4,
    "subject_must_be_shorter_than": 101,
    "body_cannot_be_empty": true
  }
}
