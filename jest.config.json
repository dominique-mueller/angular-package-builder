{
	"moduleDirectories": [
		"node_modules",
		"test/my-library/dist"
	],
	"moduleFileExtensions": [
		"js",
		"ts",
		"json"
	],
	"rootDir": ".",
	"setupTestFrameworkScriptFile": "<rootDir>/jest.setup.ts",
	"transform": {
		"^.+\\.js$": "babel-jest",
		"^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
	},
	"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
	"verbose": true
}
